{
  "_version": "1.65.0",
  "sap.app": {
    "id": "leftmenu",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.4",
      "toolsId": "fae83f84-ae99-46a5-9a85-c12e38d2b9d6"
    },
    "dataSources": {
      "mainService": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    " SalesOrderModel":{
  "type": "sap.ui.model.json.JSONModel",
  "uri": "leftmenu/mockdata/Salesorder.json",
  "dataSource":"SalesOrderJSONData",
  "settings": {
      "defaultBindingMode": "TwoWay"
    }
 }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "leftmenu.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "leftmenu.view",
        "async": true,
        "viewPath": "leftmenu.view"
        
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "pattern": "",
          "name": "Home",
          "target": "Home"
        },
        {
          "pattern": "Orders",
          "name": "Orders",
          "target": "Orders"
        },
        {
          "pattern": "Products",
          "name": "Products",
          "target": "Products"
        },
        {
          "pattern": "Detail/{productID}",
          "name": "Detail",
          "target": "Detail"
      }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
        "home": {
          "viewId": "Home",
      "viewName": "Home"
    },
    "orders": {
      "viewId": "Orders",
      "viewName": "Orders"
    },
    "products": {
      "viewId": "Products",
      "viewName": "Products"
    },
    "Detail": {
      "viewName": "Detail",
    "viewId": "Detail",
    "viewLevel": 1
    
    }
      }
    },
    "rootView": {
      "viewName": "leftmenu.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
